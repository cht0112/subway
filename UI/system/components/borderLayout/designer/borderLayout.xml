    <properties> 
      <property name="id" text="编号" required="false"/>  
      <property name="size" text="高度" use-default="true"/>
      <property name="border-size" text="外边框宽度"/> 
      <property name="style" text="样式" editor-ref="styleDialog" use-default="false"/> 
