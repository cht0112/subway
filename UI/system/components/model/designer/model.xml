<?xml version="1.0" encoding="UTF-8"?>
<elements>
	<element name="model" tag-type="rich" tag-name="model" icon="model.gif" text="数据模型" display-property="id"
			 namespace="xforms"  namespace-uri="http://www.justep.com/xforms"
			 component-class="Model"  parent-range="window"
			 component-type="model" validator="DataValidator" design-view="web-designer" >
		<properties>
			<property name="id" text="编号" required="true" />        
		</properties>
 
		<templates>
		   <template name="default"><![CDATA[ 
		     <xforms:model ></xforms:model>
		   ]]></template>
		</templates>
		
		<events>
			<event name="xforms-model-construct" display-name="onModelConstruct" type="event" text="数据模型创建时" parameters="event" is-script="true"/>
			<event name="xforms-model-construct-done" display-name="onModelConstructDone" type="event" text="数据模型创建中" parameters="event" is-script="true"/>
			<event name="xforms-ready" display-name="onReady" type="event" text="数据模型准备完成" parameters="event" is-script="true"/>
			<event name="xbl-loaded" display-name="onXBLLoaded" type="event" text="XBL组件加载完成" parameters="event" is-script="true"/>
			<event name="onload" display-name="onLoad" type="event" text="页面初始化完成" parameters="event" is-script="true"/>
			<event name="onunload" display-name="onunLoad" type="event" text="页面关闭时" parameters="event" is-script="true"/>
		</events>			
	</element>
</elements>