	<element name="button(html)" tag-name="button" icon="button.gif" text="按钮" 
		namespace="xhtml" namespace-uri="http://www.w3.org/1999/xhtml"	
		 component-class="Button" component-type="html">
		<properties>
			<property name="id" text="编号" />  
			<property name="accessKey" text="快捷键" />
			<property name="class" text="class" editor-extend-data="xui-button:button"/>
			<property name="dir" editor-ref="edit-dir" text="文本方向" />
			<property name="disabled" text="禁用" editor-ref="edit-disabled" readonly="true"/>
			<property name="for" text="绑定元素ID" />
			<property name="lang" text="语言代码" />
			<property name="name" text="名称" />
			<property name="style" text="样式" editor-ref="styleDialog"/>
			<property name="tabindex" text="Tab键顺序" data-type="integer" />
			<property name="text" text="文本" path="text()"/>    
			<property name="title" text="额外信息(可在工具提示中显示)"/>
			<property name="type" text="按钮的类型" editor-ref="edit-button-type"/>
			<property name="value" text="值" />
			<property name="xml:lang" text="XHTML语言代码" />
		</properties>

		<property-editors>
			<property-editor name="edit-button-type" type="list">
				<static-data value="button,reset,submit" />
			</property-editor>
		</property-editors>

		<events>
			<event name="onfocus" type="html" />
			<event name="onblur" type="html" />
			<event name="onclick" type="html" />
			<event name="ondblclick" type="html" />
			<event name="onmousedown" type="html" />
			<event name="onmouseup" type="html" />
			<event name="onmouseover" type="html" />
			<event name="onmousemove" type="html" />
			<event name="onmouseout" type="html" />
			<event name="onkeypress" type="html" />
			<event name="onkeydown" type="html" />
			<event name="onkeyup" type="html" />
		</events>
			
		<templates>
		   <template name="default"><![CDATA[ 
		     <xhtml:button xmlns:xhtml="http://www.w3.org/1999/xhtml">button</xhtml:button>
		   ]]></template>
		</templates>
 
	</element>
	
</elements>
